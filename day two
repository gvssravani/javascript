<!DOCTYPE html>
<html lang="en">
<head>
    OPERATORS
</head>
<body>
    <script>
        // Arithmetic 
        num1=30;
        num2=20;
        num3=20;
        num4="30";
        var A=num1+num2;
        console.log(A);
        var S=num1-num2;
        console.log(S);
        var M=num1*num2;
        console.log(M);
        var D=num1/num2;
        console.log(D);
        var Mo=num1%num2;
        console.log(Mo);
    
        //Assignment
        num1+=num1;
        console.log(num1);
        num1-=num1;
        console.log(num1);
        num1/=num1;
        console.log(num1);
        num1%=num1;
        console.log(num1);

        //Comparison
        console.log( num2==num3);
        console.log(num1==num4);
        console.log( num2!=num3);
        console.log( num2<num3);
        console.log( num2>num3);
        console.log( num2>=num3);
        console.log( num2<=num3);
        
        //Identity
        console.log(num1===num4);
        console.log(num2!==num3);
        
        //Conditional
        console.log(num2==num3 && num1==num4);
        console.log(num2==num3 || num1==num4);

        //unary
        RK1=1;
        RK2=3;
        RK1++;
        console.log(RK1);
        RK2--;
        console.log(RK2);
        ++RK1;
        console.log(RK1);
        --RK2;
        console.log(RK2);
    </script>
</body>
</html>
